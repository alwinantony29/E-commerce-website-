    <section>
      <div class="container pt-5">
        <div class="row">
          {{#each products}}

          <div class="col-md-3 p-3">
           
            <div class="card" style="width: 15rem; height: 16rem;">
              <img
                src="/product-images/{{this.product._id}}.jpg"
                class="card-img-top"
                style="height: 10rem;"
                alt="..."
              />
              <div class="card-body">
                <h5 class="card-title">{{this.product.Name}}</h5>
                <p>{{this.Price}}</p>
                <p class="card-text">Quantity:{{this.quantity}}</p>  
                
              </div>
            </div>
           </div>  
           {{/each}}        
           </div>
      </div>
    </section> 

    