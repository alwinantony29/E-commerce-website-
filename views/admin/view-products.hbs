<section>
    <div class="container mt-4">
       
        <a href="/admin/add-product" class=" btn btn-warning ml-auto" >Add products</a>
     
        <table class="table mt-5">
  <thead>
    <tr>
      <th scope="col">No</th>
      <th scope="col">Title</th>
      <th scope="col">Catagory</th>
      <th scope="col">Description</th>
      <th scope="col">Image</th>
    </tr>
  </thead>
  <tbody>
    {{#each products}}
    <tr>
      <th scope="row">{{@index}}</th>
      <td>{{this.Name}}</td>
      <td>{{this.Category}}</td>
      <td>{{this.Description}}</td>
      <td><img style="width: 100px;height:100px" src="{{this.image}}" alt="" srcset=""></td>
    </tr>
   {{/each}}
  </tbody>
</table>
    </div>
</section>