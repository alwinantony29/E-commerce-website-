<section>
    <div class="container h-80 align-items-center">
        <table class="table mt-5" id="admin-table">
            <thead>
                <tr>
                    <th scope="col">No</th>
                    <th scope="col">Name</th>
                    <th scope="col">Contact Number</th>
                    <th scope="col">Email</th>
                    {{!-- <th scope="col"></th> --}}
                </tr>
            </thead>
            <tbody>
                {{#each userList}}
                <tr>
                    <th scope="row">{{@index}}</th>
                    <td>{{this.Name}}</td>
                    <td>{{this.PhoneNumber}}</td>
                    <td>{{this.Email}}</td>
                    {{!-- <td>
                        <a href="/admin/delete-product/{{this._id}}"
                            onclick="return confirm('are you sure you want to delete {{this.Name}}')"
                            class="btn btn-danger">Delete</a>
                    </td> --}}
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</section>
<script>
    
    $(function () {
        $('#admin-table').DataTable();
    })
</script>